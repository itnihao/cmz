[program:{{project.name}}_memcached_{{vhost.ip}}_{{vhost_port}}]
command=/usr/bin/memcached -v -u nobody -l 0.0.0.0 -p {{host_port}} -m {{max_mem_size}}
priority=999
autostart=true
startretries=3
exitcodes=0,2
stopsignal=QUIT
stopwaitsecs=10
log_stdout=true
log_stderr=true
logfile_maxbytes=10MB
logfile_backups=10

